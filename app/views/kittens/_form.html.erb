<%= form_with model: @kitten, local: true do |f| %>
  <%= f.label 'Name:' %>
  <%= f.text_field :name %>
  <%= f.label 'Age:' %>
  <%= f.number_field :age %>
  <%= f.label 'Cuteness:' %>
  <%= f.number_field :cuteness %>
  <%= f.label 'Softness:' %>
  <%= f.number_field :softness %><br><br>
  <%= f.submit 'Submit' %>
<% end %>